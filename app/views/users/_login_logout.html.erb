<% if current_user %>
  <li><%= link_to "Logout", logout_path, method: :delete, class: "btn btn-primary buttons" %></li>
<% else %>
  <li><%= link_to "Sign up", new_user_path, class: "btn btn-primary buttons" %></li>
  <li><%= link_to "Login", login_path, class: "btn btn-primary buttons" %></li>
<% end %>
